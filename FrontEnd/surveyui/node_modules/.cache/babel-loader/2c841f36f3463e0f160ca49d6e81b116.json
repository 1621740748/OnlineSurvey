;AAGA,QAAA,MAAA,CAAA,KAAA,CACA,IADA,CACA,aADA,EACA,IADA,EAEA,IAFA,CAEA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,+BAAA,IAAA,CAAA,MAAA,oCADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;AAIA,SAPA,EAQA,KARA,CAQA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,EAAA,MADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;AAIA,SAbA;AAcA,OA3BA;AA4BA,KAjHA;;AAkHA;AACA;AACA;AACA,IAAA,YArHA,wBAqHA,IArHA,EAqHA;AAAA;;AACA,WAAA,QAAA,yEACA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,SAAA,CACA,CADA,EAEA,CAFA,CADA,mCAKA,IAAA,CAAA,GAAA,CAAA,QALA,oGAOA,IAPA,EAQA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA,SAHA;AAIA,QAAA,wBAAA,EAAA;AAJA,OARA,EAcA,IAdA,CAcA,YAAA;AACA,YAAA,IAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,EAAA,GAAA,IAAA,CAAA,GAAA,CAAA,EAAA;;AACA,QAAA,MAAA,CAAA,KAAA,CACA,IADA,CACA,aADA,EACA,CAAA,IAAA,CADA,EAEA,IAFA,CAEA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,wCAAA,IAAA,CAAA,GAAA,CAAA,EAAA,oCADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,aAAA;AACA,SARA,EASA,KATA,CASA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,EAAA,MADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;AAIA,SAdA;AAeA,OAhCA;AAiCA,KAvJA;;AAwJA;AACA;AACA;AACA,IAAA,cA3JA,0BA2JA,IA3JA,EA2JA;AAAA;;AACA,WAAA,QAAA,0EACA,IAAA,CAAA,MADA,uEAEA,IAFA,EAGA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA,SAHA;AAIA,QAAA,wBAAA,EAAA;AAJA,OAHA,EASA,IATA,CASA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CACA,IADA,CACA,aADA,EACA,IADA,EAEA,IAFA,CAEA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,+BAAA,IAAA,CAAA,MAAA,oCADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,aAAA;AACA,SARA,EASA,KATA,CASA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,EAAA,MADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;AAIA,SAdA;AAeA,OAzBA;AA0BA,KAtLA;;AAuLA;AACA;AACA;AACA,IAAA,aA1LA,2BA0LA;AAAA;;AACA,UAAA,IAAA,GAAA,EAAA;AACA,MAAA,IAAA,CAAA,IAAA,GAAA,KAAA,SAAA,CAAA,QAAA,CAAA,WAAA;AACA,MAAA,IAAA,CAAA,IAAA,GAAA,KAAA,SAAA,CAAA,QAAA,CAAA,QAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,WAAA,EAAA,EAAA,CAAA,SAAA,CAAA,IAAA,CAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,QAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA;AACA,OAHA;AAIA;AAlMA,GAzBA;AA8NA,EAAA,OA9NA,qBA8NA;AACA,SAAA,aAAA;AACA;AAhOA,CAAA","sourcesContent":["<template>\r\n  <div class=\"user\">\r\n    <my-table\r\n      :data=\"tableData\"\r\n      @page=\"userPageQuery\"\r\n      @delete=\"handleDelete\"\r\n      @status=\"handleStatus\"\r\n      @mulitOpen=\"handleMulitOpen\"\r\n      @mulitBan=\"handleMulitBan\"\r\n      @mulitDel=\"handleMulitDel\"\r\n    ></my-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MyTable from \"../components/Table\";\r\nimport { UserCols } from \"../components/UserCols\";\r\nimport { Toolbar } from \"../components/Toolbar\";\r\nimport { PageInfo } from \"../components/PageInfo\";\r\nimport QS from \"qs\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: {\r\n        cols: UserCols,\r\n        rowData: [],\r\n        pageinfo: PageInfo,\r\n        toolbar: Toolbar,\r\n        searchFilterConfig: {\r\n          isSearch: true,\r\n          placeholder: \"搜索ID、用户名\",\r\n          filters: [\"id\", \"username\"]\r\n        },\r\n        isOperation: true,\r\n        operation: {\r\n          label: \"操作选项\",\r\n          width: \"\",\r\n          data: [{ label: \"删除\", type: \"danger\", emit: \"delete\" }]\r\n        }\r\n      }\r\n    };\r\n  },\r\n\r\n  components: { MyTable },\r\n\r\n  methods: {\r\n    /**\r\n     * 处理单个账号状态\r\n     */\r\n    handleStatus(data) {\r\n      data.row.status === 1 ? (data.row.status = 0) : (data.row.status = 1);\r\n      let message =\r\n        data.row.status === 1\r\n          ? `确定要将 <strong>用户ID前6位: ${data.row.id.substring(\r\n              0,\r\n              6\r\n            )}, 用户名: ${\r\n              data.row.username\r\n            }</strong> 的用户<strong style=\"color:#ff6161\">封禁</strong>吗？`\r\n          : `确定要将 <strong>用户ID前6位: ${data.row.id.substring(\r\n              0,\r\n              6\r\n            )}, 用户名: ${\r\n              data.row.username\r\n            }</strong> 的用户<strong style=\"color:#ff6161\">恢复正常</strong>吗？`;\r\n      this.$confirm(message, \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n        dangerouslyUseHTMLString: true\r\n      }).then(() => {\r\n        const user = {};\r\n        user.id = data.row.id;\r\n        user.status = data.row.status === 1 ? 0 : 1;\r\n        this.$http\r\n          .post(\"user/status\", [user])\r\n          .then(() => {\r\n            data.row.status === 1\r\n              ? (data.row.status = 0)\r\n              : (data.row.status = 1);\r\n            this.$message({\r\n              message: `用户ID: ${data.row.id} 已处理`,\r\n              type: \"success\"\r\n            });\r\n          })\r\n          .catch(() => {\r\n            this.$message({\r\n              message: \"操作失败\",\r\n              type: \"error\"\r\n            });\r\n          });\r\n      });\r\n    },\r\n    /**\r\n     * 批量开启用户账号\r\n     */\r\n    handleMulitOpen(rows) {\r\n      this.$confirm(\r\n        `确定要将这 <strong style=\"color:#ff6161\">${rows.length}个用户</strong> 恢复正常状态吗？`,\r\n        \"提示\",\r\n        {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\",\r\n          dangerouslyUseHTMLString: true\r\n        }\r\n      ).then(() => {\r\n        rows.forEach(row => {\r\n          row.status = 1;\r\n        });\r\n        this.$http\r\n          .post(\"user/status\", rows)\r\n          .then(() => {\r\n            this.$message({\r\n              message: `已将这 ${rows.length}个用户 恢复正常状态`,\r\n              type: \"success\"\r\n            });\r\n          })\r\n          .catch(() => {\r\n            this.$message({\r\n              message: \"操作失败\",\r\n              type: \"error\"\r\n            });\r\n          });\r\n      });\r\n    },\r\n    /**\r\n     * 批量封禁用户账号\r\n     */\r\n    handleMulitBan(rows) {\r\n      this.$confirm(\r\n        `确定要将这 <strong style=\"color:#ff6161\">${rows.length}个用户</strong> 封禁吗？`,\r\n        \"提示\",\r\n        {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\",\r\n          dangerouslyUseHTMLString: true\r\n        }\r\n      ).then(() => {\r\n        rows.forEach(row => {\r\n          row.status = 0;\r\n        });\r\n        this.$http\r\n          .post(\"user/status\", rows)\r\n          .then(() => {\r\n            this.$message({\r\n              message: `已将这 ${rows.length}个用户 封禁`,\r\n              type: \"success\"\r\n            });\r\n          })\r\n          .catch(() => {\r\n            this.$message({\r\n              message: \"操作失败\",\r\n              type: \"error\"\r\n            });\r\n          });\r\n      });\r\n    },\r\n    /**\r\n     * 删除单个记录\r\n     */\r\n    handleDelete(data) {\r\n      this.$confirm(\r\n        `确定要将 <strong>用户ID前6位: ${data.row.id.substring(\r\n          0,\r\n          6\r\n        )}, 用户名: ${\r\n          data.row.username\r\n        }</strong> 的用户<strong style=\"color:#ff6161\">删除</strong>吗？`,\r\n        \"提示\",\r\n        {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\",\r\n          dangerouslyUseHTMLString: true\r\n        }\r\n      ).then(() => {\r\n        const user = {};\r\n        user.id = data.row.id;\r\n        this.$http\r\n          .post(\"user/delete\", [user])\r\n          .then(() => {\r\n            this.$message({\r\n              message: `已将用户ID: ${data.row.id} 的用户删除`,\r\n              type: \"success\"\r\n            });\r\n            this.userPageQuery();\r\n          })\r\n          .catch(() => {\r\n            this.$message({\r\n              message: \"操作失败\",\r\n              type: \"error\"\r\n            });\r\n          });\r\n      });\r\n    },\r\n    /**\r\n     * 批量删除用户账号\r\n     */\r\n    handleMulitDel(rows) {\r\n      this.$confirm(\r\n        `确定要将这 <strong style=\"color:#ff6161\">${rows.length}个用户</strong> 永久删除吗？`,\r\n        \"提示\",\r\n        {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\",\r\n          dangerouslyUseHTMLString: true\r\n        }\r\n      ).then(() => {\r\n          this.$http\r\n          .post(\"user/delete\", rows)\r\n          .then(() => {\r\n            this.$message({\r\n              message: `已将这 ${rows.length}个用户 删除`,\r\n              type: \"success\"\r\n            });\r\n            this.userPageQuery();\r\n          })\r\n          .catch(() => {\r\n            this.$message({\r\n              message: \"操作失败\",\r\n              type: \"error\"\r\n            });\r\n          });\r\n      });\r\n    },\r\n    /**\r\n     * 获取表格数据\r\n     */\r\n    userPageQuery() {\r\n      const page = {};\r\n      page.page = this.tableData.pageinfo.currentPage;\r\n      page.size = this.tableData.pageinfo.pageSize;\r\n      this.$http.post(\"user/list\", QS.stringify(page)).then(res => {\r\n        this.tableData.pageinfo.total = res.data.count;\r\n        this.tableData.rowData = res.data.userList;\r\n      });\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.userPageQuery();\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/admin/user"}]}